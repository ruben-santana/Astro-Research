;Radians to Degrees Function
pro r_t_d, d  ;d = degrees

d = (r*360.)/(2.*pi)

return d

end


;Degress to Radians Function
pro d_t_r, d
;d = de

r = (d*2.*pi)/(360.)

return r

end


;Weight on Mars Fuction
pro weight_of_mars, m
;lb = weight on Mars & m = mass

lb = (3.711*m)*2.2046226

return lb

end 


;Tip Calculator Procedure
pro tip_calc, a, g
;a = amount of the check & g = group size

tip = (a*.15)
total = tip + a
tpp = tip / g ;tpp = tip per person
all = [tip, tpp, total]

return all

end


